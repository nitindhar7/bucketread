<%= render :partial => "shared/nav" %>

<% form_tag authorize_path do %>
	<%= label_tag :email %>
	<%= text_field_tag :email, params[:email], :class => "field" %>
	<%= label_tag :password %>
	<%= password_field_tag :password, nil, :class => "field" %>
	<%= submit_tag "Login", :class => "button" %>
<% end %>